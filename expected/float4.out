-- special inputs
BEGIN;
DELETE FROM FLOAT4_TBL;
INSERT INTO FLOAT4_TBL(f1) VALUES ('NaN'::float4);
INSERT INTO FLOAT4_TBL(f1) VALUES ('nan'::float4);
INSERT INTO FLOAT4_TBL(f1) VALUES ('   NAN  '::float4);
INSERT INTO FLOAT4_TBL(f1) VALUES ('infinity'::float4);
INSERT INTO FLOAT4_TBL(f1) VALUES ('          -INFINiTY   '::float4);
